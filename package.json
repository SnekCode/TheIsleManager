{
  "name": "theislemanager",
  "description": "A Dino Game Manager for The Isle",
  "version": "1.1.1",
  "author": "SnekCode",
  "license": "ISC",
  "main": "dist-electron/main/main.js",
  "repository": "https://github.com/SnekCode/TheIsleManager",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:updater": "set VITE_UPDATER=true && vite",
    "build": "tsc && vite build && electron-builder --publish never",
    "start": "electron .",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "publish": "tsc && vite build && electron-builder --publish always"
  },
  "dependencies": {
    "electron-forge": "^5.2.4",
    "electron-log": "^5.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.4",
    "less": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "27.0.4",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
